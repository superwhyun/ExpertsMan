{
  "name": "expertsman-api",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev --ip 127.0.0.1 --port 8787 --var CORS_ORIGIN:http://localhost:5173",
    "dev:local": "wrangler dev --local --ip 127.0.0.1 --port 8787 --var CORS_ORIGIN:http://localhost:5173",
    "deploy": "wrangler deploy",
    "db:migrate": "wrangler d1 execute expertsman-db --local --file=./migrations/0001_initial_schema.sql",
    "db:migrate:prod": "wrangler d1 execute expertsman-db --remote --file=./migrations/0001_initial_schema.sql",
    "start:legacy": "node server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "ExpertsMan API - Cloudflare Workers",
  "dependencies": {
    "hono": "^4.6.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241205.0",
    "typescript": "^5.7.0",
    "wrangler": "^3.99.0"
  },
  "legacyDependencies": {
    "cors": "^2.8.6",
    "dotenv": "^17.2.4",
    "express": "^5.2.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7"
  }
}
